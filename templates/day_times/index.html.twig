{% extends 'base.html.twig' %}

{% block title %}DayTimes index{% endblock %}

{% block body %}
    <h1>DayTimes index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Day_time</th>
                <th>Photo</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for day_time in day_times %}
            <tr>
                <td>{{ day_time.id }}</td>
                <td>{{ day_time.dayTime }}</td>
                <td>{{ day_time.photo }}</td>
                <td>
                    <a href="{{ path('app_day_times_show', {'id': day_time.id}) }}">show</a>
                    <a href="{{ path('app_day_times_edit', {'id': day_time.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_day_times_new') }}">Create new</a>
{% endblock %}
